<launch>
	<node pkg="simple_arm_control" type="move_right_arm" name="move_arm_to_position" output="screen">
     
     <param name="planner_service_name" value="sbpl_planning/plan_path"/>
	   <param name="goal_x" value="0.585"    type="double"/>
	   <param name="goal_y" value="-0.4868"  type="double"/>
	   <param name="goal_z" value="0.801"     type="double"/>

	   <param name="goal_roll" value="0.6391"  type="double"/>
	   <param name="goal_yaw" value="0.2657"    type="double"/>
	   <param name="goal_pitch" value="-0.3554"  type="double"/>
     <param name="planning_duration" value="20.0" type="double" />

	</node>
</launch>
